        },
        "cjk_charfilter": {
            "mappings": [
                "\u309b=>\\u3099",
                "\u309c=>\\u309a"
            ],
            "type": "mapping"
        },
        "dotted_I_fix": {
            "mappings": [
                "\u0130=>I"
            ],
            "type": "mapping"
        },
        "nnbsp_norm": {
            "mappings": [
                "\\u202F=>\\u0020"
            ],
            "type": "mapping"
        },
        "split_camelCase": {
            "pattern": "(?<=\\p{Ll}[\\p{M}\\p{Cf}]{0,9}+)(\\p{Lu})",
            "replacement": " $1",
            "type": "pattern_replace"
        },
        "thai_charfilter": {
            "mappings": [
                "_=>\\u0020",
                ";=>\\u0020",
                ":=>\\u0020",
                "\u00b7=>\\u0020",
                "\u2027=>\\u0020",
                "\u0e03=>\u0e02",
                "\u0e05=>\u0e04",
                "\\u0e4d\\u0e32=>\\u0e33",
                "\\u0e4d\\u0e48\\u0e32=>\\u0e48\\u0e33",
                "\\u0e4d\\u0e49\\u0e32=>\\u0e49\\u0e33",
                "\\u0e33\\u0e48=>\\u0e48\\u0e33",
                "\\u0e33\\u0e49=>\\u0e49\\u0e33",
                "\\u0E34\\u0E4D=>\\u0E36",
                "\\u0E4D\\u0E34=>\\u0E36"
            ],
            "type": "mapping"
        },
        "thai_repl_pat": {
            "pattern": "(?<=\\p{Nd})([\u0e01-\u0e4f])|(?<=[\u0e01-\u0e4f])(\\p{Nd})|(?<=[\u0e01-\u0e4f])\\.([\u0e01-\u0e4f])",
            "replacement": " $1$2$3",
            "type": "pattern_replace"
        },
        "word_break_helper": {
            "mappings": [
