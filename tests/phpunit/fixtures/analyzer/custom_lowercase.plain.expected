                "icu_normalizer"
            ],
            "tokenizer": "standard",
            "type": "custom"
        },
        "ga_plain": {
            "char_filter": [
                "nnbsp_norm",
                "word_break_helper"
            ],
            "filter": [
                "ga_lowercase",
                "icu_normalizer",
                "preserve_original_recorder",
                "icu_folding",
                "preserve_original",
                "remove_empty"
            ],
            "tokenizer": "standard",
            "type": "custom"
        },
        "ga_plain_search": {
            "char_filter": [
                "nnbsp_norm",
                "word_break_helper"
            ],
            "filter": [
                "ga_lowercase",
                "icu_normalizer"
            ],
            "tokenizer": "standard",
            "type": "custom"
        },
        "tr_plain": {
            "char_filter": [
                "nnbsp_norm",
                "word_break_helper"
            ],
            "filter": [
                "tr_lowercase",
                "icu_normalizer",
                "preserve_original_recorder",
                "tr_icu_folding",
                "preserve_original",
                "remove_empty"
            ],
            "tokenizer": "standard",
