            "filter": [
                "tr_lowercase",
                "icu_normalizer"
            ],
            "tokenizer": "standard",
            "type": "custom"
        }
    },
    "char_filter": {
        "nnbsp_norm": {
            "mappings": [
                "\\u202F=>\\u0020"
            ],
            "type": "mapping"
        },
        "word_break_helper": {
            "mappings": [
                "_=>\\u0020",
                ".=>\\u0020",
                "(=>\\u0020",
                ")=>\\u0020"
            ],
            "type": "mapping"
        }
    },
    "filter": {
        "ga_lowercase": {
            "language": "irish",
            "type": "lowercase"
        },
        "icu_folding": {
            "type": "icu_folding"
        },
        "icu_normalizer": {
            "name": "nfkc_cf",
            "type": "icu_normalizer"
        },
        "lowercase": {
            "language": "greek",
            "type": "lowercase"
        },
        "remove_empty": {
            "min": 1,
            "type": "length"
        },
        "tr_icu_folding": {
            "type": "icu_folding",
            "unicodeSetFilter": "[^\u00c7\u00e7\u011e\u011fI\u0131\u0130i\u00d6\u00f6\u015e\u015f\u00dc\u00fc]"
        },
        "tr_lowercase": {
            "language": "turkish",
            "type": "lowercase"
        }
    },
    "tokenizer": []
}