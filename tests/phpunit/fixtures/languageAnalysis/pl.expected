        "prefix_ngram_filter": {
            "type": "edgeNGram",
            "max_gram": 255
        },
        "remove_empty": {
            "type": "length",
            "min": 1
        },
        "stempel_pattern_filter": {
            "type": "pattern_replace",
            "pattern": "^([a-z\u0105\u0142]?[a-z\u0107\u0144]|..\u0107|\\d.*\u0107)$",
            "replacement": ""
        },
        "stempel_stop": {
            "type": "stop",
            "stopwords": [
                "owa\u0107",
                "iwa\u0107",
                "obi\u0107",
                "sn\u0105\u0107",
                "ywa\u0107",
                "ium",
                "my",
                "um"
            ]
        },
        "suggest_shingle": {
            "type": "shingle",
            "min_shingle_size": 2,
            "max_shingle_size": 3,
            "output_unigrams": true
        },
        "truncate_keyword": {
            "type": "truncate",
            "length": 5000
        }
    },
    "tokenizer": {
        "no_splitting": {
            "type": "keyword"
        },
        "prefix": {
            "type": "edgeNGram",
            "max_gram": 255
        },
        "trigram": {
            "type": "nGram",
            "min_gram": 3,
            "max_gram": 3
        }
    }
}